package server;

public class CounterProtocol {

	public String process(String command) {
		String[] items = command.split("\\s");
		if (items.length < 2) {
			return "Command did not have enough params";
		} else {
			switch (items[0]) {
			case value:
				
				break;

			default:
				break;
			}
		}
	}

}
